{
    "version": 2,
    "builds": [
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/main.py"
      },
      {
        "src": "/(.*\\..*)",
        "dest": "frontend/dist/frontend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/dist/frontend/index.html"
      }
    ]
  }
  